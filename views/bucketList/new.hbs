<h1 class="edith1">Create Bucket List</h1>
<div class="row">
    <div class="container">
        <form method='POST' action='/users/{{userId}}/bucketLists'>
            <input type='text' name='name' placeholder='Name of Bucket List'>
            <input type='text' name='description' placeholder='Description'>
            <input type='text' name='totalCost' placeholder='Total Cost'>
            <div class="alignCenter">
                <input type='submit' class="waves-effect waves-light btn #00838f cyan darken-3" value='Create Bucket List'>
            </div>
        </form>
    </div>
</div>
<div class="alignCenter">
    <a href='/users/{{userId}}/bucketLists' class="waves-effect waves-light btn #00838f cyan darken-3">Go Back</a>
</div>